{
  "name": "@phonofidelic/tapes-desktop-client",
  "productName": "Tapes",
  "version": "1.0.1",
  "description": "My Electron application description",
  "main": "./.webpack/main",
  "scripts": {
    "install": "TARGET_ARCH=x64",
    "start": "DEBUG='electron-forge:*' NODE_ENV=development electron-forge start --inspect-electron",
    "start:web": "npx webpack serve --mode development --config webpack.web-client.config.js",
    "build:web": "npx webpack --config webpack.web-client.config.js",
    "package": "ACOUSTID_API_KEY=$(grep ACOUSTID_API_KEY .env | cut -d '=' -f2) electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "publish-local": "GITHUB_TOKEN=$(grep GITHUB_TOKEN .env | cut -d '=' -f2) electron-forge publish",
    "lint": "eslint --ext .ts .",
    "test": "jest --watch --config jest.config.js",
    "test:ci": "jest",
    "test:e2e": "yarn package && jest --config jest.e2e.config.js",
    "test:e2e:nopack": "jest --detectOpenHandles --forceExit --config jest.e2e.config.js",
    "test:db": "jest --watch --detectOpenHandles --config jest.db.config.js",
    "icon": "./node_modules/.bin/electron-icon-builder --input=/Users/christopherclemons/Projects/Sandbox/try_electron-forge/src/assets/icon@16.png --output=./assets",
    "dep": "arkit -c arkit.json"
  },
  "keywords": [],
  "author": {
    "name": "phonofidelic",
    "email": "chris.clemons85@gmail.com"
  },
  "resolutions": {
    "electron": "^17.0.0",
    "orbit-db-io": "^1.0.2"
  },
  "license": "MIT",
  "release": {
    "branches": [
      "main",
      {
        "name": "development",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ]
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/icons/mac/icon.icns",
        "extraResource": [
          "bin",
          "src/assets/icon@16.png",
          "preload.js",
          "node_modules"
        ],
        "asar": false
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tapes_electron_client"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src 'self' * 'unsafe-eval'",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "phonofidelic",
              "name": "tapes-electron-client"
            },
            "prerelease": true
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@electron-forge/cli": "^6.0.0-beta.63",
